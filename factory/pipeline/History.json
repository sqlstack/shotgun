{
	"name": "History",
	"properties": {
		"description": "Get historical data of taxi trips",
		"activities": [
			{
				"name": "Copy_CSV",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "yellow_tripdata_2009-01.csv"
					},
					{
						"name": "Destination",
						"value": "history//yellow_tripdata_2009-01.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Shotgun_Lake",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "NYC_Taxi",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}