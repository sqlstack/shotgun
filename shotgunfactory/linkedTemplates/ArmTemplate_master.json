{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shotgunfactory"
		},
		"shotgunlake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'shotgunlake'"
		},
		"ShotgunSQLDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ShotgunSQLDatabase'"
		},
		"NYC HTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://s3.amazonaws.com/nyc-tlc/trip+data/"
		},
		"shotgunlake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shotgunlake.dfs.core.windows.net"
		},
		"shotgunKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://shotgunKeyVault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"shotgunlake_accountKey": {
						"value": "[parameters('shotgunlake_accountKey')]"
					},
					"ShotgunSQLDatabase_connectionString": {
						"value": "[parameters('ShotgunSQLDatabase_connectionString')]"
					},
					"NYC HTTP_properties_typeProperties_url": {
						"value": "[parameters('NYC HTTP_properties_typeProperties_url')]"
					},
					"shotgunlake_properties_typeProperties_url": {
						"value": "[parameters('shotgunlake_properties_typeProperties_url')]"
					},
					"shotgunKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('shotgunKeyVault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}